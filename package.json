{
  "name": "nextjs-aikeedo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:build": "tsc --noEmit --exclude '**/*.test.ts' --exclude '**/*.spec.ts' --exclude '**/tests/**'",
    "verify": "node scripts/verify-setup.js",
    "test-build": "bash scripts/test-build.sh",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "dev:demo": "tsx scripts/dev-utils-demo.ts",
    "test-db:start": "bash scripts/test-db.sh start",
    "test-db:stop": "bash scripts/test-db.sh stop",
    "test-db:restart": "bash scripts/test-db.sh restart",
    "test-db:reset": "bash scripts/test-db.sh reset",
    "test-db:logs": "bash scripts/test-db.sh logs",
    "test-db:status": "bash scripts/test-db.sh status",
    "config:view": "tsx scripts/config-manager.ts view",
    "config:validate": "tsx scripts/config-manager.ts validate",
    "config:init": "tsx scripts/config-manager.ts init",
    "config:diff": "tsx scripts/config-manager.ts diff",
    "config:help": "tsx scripts/config-manager.ts help",
    "config:demo": "tsx scripts/demo-config.ts",
    "find:hardcoded": "tsx scripts/find-hardcoded-values.ts",
    "credits:add": "tsx scripts/add-credits.ts",
    "credits:list": "tsx scripts/add-credits.ts --list"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@google/generative-ai": "^0.24.1",
    "@mistralai/mistralai": "^1.10.0",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.0.1",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@tanstack/react-query": "^5.90.11",
    "bcrypt": "^6.0.0",
    "next": "14.2.33",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.11",
    "openai": "^6.9.1",
    "prisma": "^7.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "redis": "^5.10.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "stripe": "^20.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.25",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/stripe": "^8.0.416",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^4.3.0",
    "pg": "^8.16.3",
    "postcss": "^8",
    "prettier": "^3.7.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.14",
    "ws": "^8.18.3"
  }
}
